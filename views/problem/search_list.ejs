<main id="main_background" class="page faq-page">
    <section class="clean-block clean-faq dark" style="height: 100%;">
        <div class="container">
            <div class="row">

                <div class="col-lg-12">
                    <div id="top_text" class="text-center">
                    <p style="font-size:50px; font-family: 'NanumSquareRoundB'">통 합 검 색</p>
                    </div>

                    <div id="keyword_search">
                        <form action="" style:"width: 100%;">
                            <div class="input-group">
                                <input id="search" name="search" type="text" class="form-control"
                                       placeholder="키워드를 입력해주세요." autofocus>
                                <div class="input-group-btn">
                                    <button id="search_btn" class="btn btn-default" type="submit"
                                            style="background: #eb6f65; border-radius: 0;"><i class="fas fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>

                <hr style="width: 100%;">
                
                <div class="col-lg-12">
                    <div id="my_list">
                        <%
                            var keynum = 0;
                            if(result.length == 0) {
                                %>
                            검색결과가 없습니다.
                        <%
                            } else {
                            for (var i = 0; i < result.length; i++) {
                                for(var j =0; j < result2.length; j++) {
                                    if(result2[j].book_num == result[i].id_num) {
                                        keynum += 1;
                                    }
                                }
                        %>
                            <div class="list_box" style="cursor:pointer;" onclick="location.href='/problem?book_num=<%= result[i].id_num; %>'">
                                <p class="box_in_number"><%= keynum; %> <span style="font-size: 20px; color: #000000">key</span></p>
                                <p class="box_in_title"><%= result[i].title; %></p>
                                <p class="box_in_subtitle"><%= result[i].sub_title; %></p>
                                <p class="box_in_name"><%= result[i].date %>, <%= sess.name %> 님 제작</p>
                            </div>
                            <br style="height: 10px;">
                        <%
                            keynum = 0;
                            }
                        }
                        %>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </section>
</main>

